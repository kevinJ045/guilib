{
  "name": "rayous",
  "version": "1.1.9",
  "author": "makano",
  "description": "A mini router X ui kit with ts",
  "main": "./client/index.ts",
  "exports": {
    ".": "./client/index.ts",
    "./extra": "./client/extra.ts",
    "./components": "./components/index.ts",
    "./server": "./server"
  },
  "files": [
    "client/",
    "server/",
    "components/",
    "bin/",
    "extra/",
    "types/",
    "index.ts",
    "index.d.ts"
  ],
  "bin": {
    "rayous": "./bin/rayous.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kevinj045/guilib.git"
  },
  "scripts": {
    "start": "bun --watch run ./server/server.ts",
    "startBuild": "/usr/bin/webpack --config webpack.config.js --stats-error-details",
    "dev": "npm run start & npm run startBuild",
    "bun": "bun build ./src/main.js --outdir ./public/js --watch & npm run start"
  },
  "keywords": [],
  "license": "ISC",
  "type": "module",
  "typings": "./types/index.d.ts",
  "dependencies": {
    "bun": "^1.0.7",
    "console": "^0.7.2",
    "itty-router": "^4.0.23",
    "postcss": "^8.4.31",
    "sass": "^1.64.1",
    "schema-utils": "^4.2.0",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.1.6"
  }
}
